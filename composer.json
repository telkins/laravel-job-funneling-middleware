{
    "name": "telkins/laravel-job-funneling-middleware",
    "description": "A middleware that can funnel jobs.",
    "keywords": [
        "laravel-job-funneling-middleware"
    ],
    "homepage": "https://github.com/telkins/laravel-job-funneling-middleware",
    "license": "MIT",
    "authors": [
        {
            "name": "Travis Elkins",
            "email": "travis.elkins+github@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/redis": "^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0",
        "symfony/var-dumper": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Telkins\\JobFunnelingMiddleware\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Telkins\\JobFunnelingMiddleware\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
